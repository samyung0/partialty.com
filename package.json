{
  "name": "partialty",
  "description": "Online E-Learning platform for coding",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "pre-commit": [
    "lint.fix",
    "fmt",
    "git:add"
  ],
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "compile.chokidar": "del-cli ./src/assets/chokidar/index.txt && ncc build ./src/$$chokidarEntry.js -o ./src/assets/chokidar -m && rname ./src/assets/chokidar/index.js ./src/assets/chokidar/index.txt",
    "db:dev": "drizzle-kit studio --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:check": "drizzle-kit check:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:migrate": "cd ./drizzle_turso && bun migrate.dev.ts",
    "db:dev:drop": "drizzle-kit drop --config drizzle_turso/drizzle.dev.config.ts",
    "db:prod": "drizzle-kit studio --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:migrate": "cd ./drizzle_turso && bun migrate.prod.ts",
    "db:prod:drop": "drizzle-kit drop --config drizzle_turso/drizzle.prod.config.ts",
    "deploy": "vercel deploy",
    "dev": "concurrently -c \"auto\" --handle-input \"vite --mode ssr\" \"bun --hot bun/index.ts\"",
    "start:server": "bun bun/index.ts",
    "dev:server": "bun --watch bun/index.ts",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev:email": "email dev",
    "fmt": "eslint \"src/**/*.ts*\" --fix && prettier --write .",
    "fmt.check": "prettier --check .",
    "git:add": "git add .",
    "lint": "eslint \"src/**/*.ts*\" && tsc --noEmit",
    "lint.fix": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US,zh-HK,ja-JP --assetsPath=i18n",
    "serve": "node server/entry.express"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@builder.io/qwik": "^1.3.2",
    "@builder.io/qwik-city": "https://github.com/samyung0/qwikcity-dist",
    "@types/babel__generator": "^7.6.5",
    "@types/babel__traverse": "^7.20.2",
    "@types/bun": "latest",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.44.8",
    "@types/express": "^4.17.19",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mime-db": "^1.43.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vercel/ncc": "^0.38.0",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "daisyui": "^3.7.3",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-plugin-qwik": "^1.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "express": "4.18.2",
    "monaco-editor": "^0.43.0",
    "postcss": "8.4.27",
    "pre-commit": "^1.2.2",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "qwik-speak": "^0.18.0",
    "react": "^18.2.0",
    "rename-cli": "^7.0.2",
    "shikiji": "^0.9.17",
    "tailwindcss": "3.3.3",
    "typescript": "^5.3.2",
    "undici": "5.22.1",
    "vite": "^5.0.6",
    "vite-plugin-compile-time": "^0.2.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@babel/parser": "^7.23.0",
    "@builder.io/qwik-react": "^0.5.0",
    "@cloudinary/url-gen": "^1.15.0",
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/cron": "^0.8.0",
    "@elysiajs/eden": "^0.8.1",
    "@getbrevo/brevo": "^2.0.0-beta.4",
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@lucia-auth/oauth": "^3.5.0",
    "@react-email/components": "0.0.12",
    "@types/react-dom": "^18.2.18",
    "@webcontainer/api": "1.1.8",
    "axios": "^1.4.0",
    "comlink": "^4.4.1",
    "drizzle-orm": "^0.29.1",
    "elysia": "^0.8.8",
    "elysia-compression": "^0.0.7",
    "elysia-helmet": "^1.0.1",
    "elysia-rate-limit": "^2.0.1",
    "fuse.js": "^7.0.0",
    "lodash.isequal": "^4.5.0",
    "lucia": "^2.7.6",
    "mime-db": "^1.52.0",
    "octokit": "^3.1.2",
    "qwik-ionicons": "^1.0.5",
    "qwik-transition": "^0.0.7",
    "react-dom": "^18.2.0",
    "react-dropzone-esm": "^15.0.1",
    "react-email": "1.10.0",
    "react-player": "^2.14.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "module": "index.ts"
}
