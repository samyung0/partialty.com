{
  "name": "partialty",
  "description": "Online E-Learning platform for coding",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "pre-commit": [
    "fmt",
    "lint.fix"
  ],
  "scripts": {
    "test": "echo test",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint.fix": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en,zh-HK,ja --assetsPath=i18n",
    "compile.chokidar": "del-cli ./src/assets/chokidar/index.txt && ncc build ./src/$$chokidarEntry.js -o ./src/assets/chokidar -m && rname ./src/assets/chokidar/index.js ./src/assets/chokidar/index.txt"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.7",
    "@builder.io/qwik-city": "https://github.com/samyung0/qwikcity-dist/",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.44.1",
    "@types/express": "4.17.17",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vercel/ncc": "^0.38.0",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "daisyui": "^3.7.3",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "^1.2.7",
    "eslint-plugin-unused-imports": "^3.0.0",
    "express": "4.18.2",
    "monaco-editor": "^0.43.0",
    "postcss": "8.4.27",
    "pre-commit": "^1.2.2",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rename-cli": "^7.0.2",
    "supabase": "^1.86.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vercel": "^29.1.1",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@supabase/supabase-js": "^2.32.0",
    "@upstash/redis": "^1.22.0",
    "@webcontainer/api": "^1.1.5",
    "axios": "^1.4.0",
    "comlink": "^4.4.1",
    "lodash.isequal": "^4.5.0",
    "mime-types": "^2.1.35",
    "octokit": "^3.1.1",
    "qwik-ionicons": "^1.0.5",
    "qwik-speak": "^0.15.3",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
}
