{
  "name": "partialty",
  "description": "Online E-Learning platform for coding",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "pre-commit": [
    "fmt",
    "lint.fix",
    "git:add"
  ],
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "compile.chokidar": "del-cli ./src/assets/chokidar/index.txt && ncc build ./src/$$chokidarEntry.js -o ./src/assets/chokidar -m && rname ./src/assets/chokidar/index.js ./src/assets/chokidar/index.txt",
    "db:dev": "drizzle-kit studio --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:dev:push": "drizzle-kit push:sqlite --config drizzle_turso/drizzle.dev.config.ts",
    "db:prod": "drizzle-kit studio --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:generate": "drizzle-kit generate:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:pull": "drizzle-kit introspect:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "db:prod:push": "drizzle-kit push:sqlite --config drizzle_turso/drizzle.prod.config.ts",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev:email": "email dev",
    "fmt": "eslint \"src/**/*.ts*\" --fix && prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\" && tsc --noEmit",
    "lint.fix": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US,zh-HK,ja-JP --assetsPath=i18n",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "git:add": "git add ."
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.3.2",
    "@builder.io/qwik-city": "https://github.com/samyung0/qwikcity-dist",
    "@types/babel__generator": "^7.6.5",
    "@types/babel__traverse": "^7.20.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.44.8",
    "@types/express": "4.17.17",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mime-db": "^1.43.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vercel/ncc": "^0.38.0",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "daisyui": "^3.7.3",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "eslint-plugin-qwik": "^1.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "express": "4.18.2",
    "monaco-editor": "^0.43.0",
    "postcss": "8.4.27",
    "pre-commit": "^1.2.2",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "qwik-speak": "^0.18.0",
    "react": "^18.2.0",
    "rename-cli": "^7.0.2",
    "supabase": "^1.115.4",
    "tailwindcss": "3.3.3",
    "typescript": "^5.3.2",
    "undici": "5.22.1",
    "vercel": "^29.1.1",
    "vite": "^5.0.6",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@babel/parser": "^7.23.0",
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@lucia-auth/oauth": "^3.5.0",
    "@react-email/components": "0.0.12",
    "@supabase/realtime-js": "^2.8.4",
    "@supabase/supabase-js": "^2.32.0",
    "@upstash/redis": "^1.22.0",
    "@webcontainer/api": "1.1.8",
    "axios": "^1.4.0",
    "comlink": "^4.4.1",
    "drizzle-orm": "^0.29.1",
    "lodash.isequal": "^4.5.0",
    "lucia": "^2.7.6",
    "mime-db": "^1.52.0",
    "octokit": "^3.1.1",
    "qwik-ionicons": "^1.0.5",
    "react-email": "1.10.0",
    "shiki": "^0.14.7",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
}
