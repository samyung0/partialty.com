---
const dark = Astro.url.searchParams.get("dark");
---

<html class={dark ? "dark" : ""}>
  <body class={"bg-transparent dark:text-background-light-gray text-primary-dark-gray"}>
    <b>This is bold</b>
    <strong>This is also bold</strong>
  </body>
</html>

<style>
  @tailwind utilities;
</style>
